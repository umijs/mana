{
  "private": true,
  "name": "mana",
  "version": "0.1.0",
  "description": "Mana",
  "repository": "git@github.com:umijs/mana.git",
  "workspaces": [
    "packages/*",
    "dev-packages/*",
    "examples/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "prepare": "lerna run build && lerna run lint",
    "site": "dumi dev",
    "site:build": "dumi build",
    "changelog": "lerna-changelog",
    "clean": "yarn build:clean && yarn lint:clean && yarn test:clean && lerna run clean",
    "clean:all": "yarn clean && lerna clean -y",
    "build": "lerna run build",
    "build:clean": "rimraf .umi",
    "lint": "lerna run lint",
    "lint:clean": "rimraf .eslintcache",
    "test": "jest --coverage",
    "test:clean": "rimraf coverage",
    "publish:latest": "lerna publish",
    "publish:next": "lerna publish --canary=next --dist-tag=next",
    "postpublish": "manarun-check-pub"
  },
  "devDependencies": {
    "@types/assert": "^1.5.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.17.3",
    "@types/react-test-renderer": "^17.0.1",
    "@types/temp": "^0.8.34",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "@umijs/fabric": "^2.3.1",
    "antd": "^4.16.6",
    "concurrently": "^5.3.0",
    "dumi": "^1.1.18",
    "dumi-theme-default": "^1.1.1",
    "eslint": "^7.15.0",
    "eslint-plugin-deprecation": "^1.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-no-null": "^1.0.2",
    "father": "^2.28.0",
    "husky": "^5.1.3",
    "jest": "^27.0.6",
    "lerna": "^3.6.0",
    "lerna-changelog": "^2.2.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "temp": "^0.9.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  }
}
